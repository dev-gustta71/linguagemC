#include <stdio.h>
#include <conio.h>
	
	void calculadora(int num){
		int resultado;
		int i;
		
		for (i = 1 ; i <=10 ; i++){
			resultado = num * i;
			printf("%d x %d = %d \n",num,i,resultado);
		}
	}
	
	int main(){
		
		int numero;
		
		printf("Digite o numero que deseja saber: ");
		scanf("%d",&numero);
		
		calculadora(numero);
		
		
		
		
		return 0;
	}
